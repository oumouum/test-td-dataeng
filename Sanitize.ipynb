import sys
source = sys.argv[1]
target = sys.argv[2]
with open(source, 'r') as i, open(target, 'w') as o:
	o.truncate()
	p = i.read()
	print("The input are..")
	print(p)
	p = p.lower().replace('\t','____')
	o.write(p)
	o.close()
print("aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa")
count= {}
for input in p:
    	if input in count:
        	count[input] = count[input] + 1
    	else:
        	count[input] = 1

out = open(target,"a")
out.write(f"\n\nCount number of occurrence of each alphabet are {count}\n")
out.close()

out = open(target,"r")
print("The Output are..")
print(out.read())
out.close()


